<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".carros_agregar_admin">

    <include
        layout="@layout/activity_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <include
        layout="@layout/activity_nav"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="377dp"
        android:layout_height="341dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.676"
        app:srcCompat="@drawable/card_citas" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Carros"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.73" />

    <TextView
        android:id="@+id/txvVerVehiculos_agr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver vehiculos"
        android:textColor="#6E6E6E"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/txvAgregarVehiculo_agr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar vehiculos"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.683"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvAgregarVehiculo_agr"
        app:srcCompat="@drawable/ic_circle_carros" />

    <ScrollView
        android:layout_width="377dp"
        android:layout_height="341dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/imageView20">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/txtAddPlacas"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Numero de placas"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddMarca"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.622" />

            <EditText
                android:id="@+id/txtAddMarca"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:layout_marginStart="16dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Marca"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddModelo"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.819" />

            <EditText
                android:id="@+id/txtAddModelo"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Modelo y AÃ±o"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddColor"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.886" />

            <EditText
                android:id="@+id/txtAddPropietario"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Nombre del propietario"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddDui"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.937" />

            <EditText
                android:id="@+id/txtAddDui"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Dui del propietario"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddServicio"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.947" />

            <EditText
                android:id="@+id/txtAddServicio"
                android:layout_width="344dp"
                android:layout_height="73dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Descripcion del servicio que se realizara"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.484"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="384dp" />

            <EditText
                android:id="@+id/txtAddColor"
                android:layout_width="240dp"
                android:layout_height="40dp"
                android:background="@drawable/imputtext"
                android:ems="10"
                android:hint="Color"
                android:inputType="text"
                android:paddingStart="14dp"
                app:layout_constraintBottom_toTopOf="@+id/txtAddPropietario"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.921" />

            <Button
                android:id="@+id/btnRegistrarNuevoCarro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="500dp"
                android:layout_marginBottom="30dp"
                android:padding="20dp"
                android:text="Registrar"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.898"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>